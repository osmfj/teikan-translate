      <tuv lang="JA">
        <seg>&lt;f0&gt;(&lt;/f0&gt;&lt;f1&gt;目的&lt;/f1&gt;&lt;f2&gt;)&lt;br3/&gt;</seg>
      </tuv>
      <tuv lang="EN-GB" changeid="miurahr" changedate="20141229T011439Z" creationid="miurahr" creationdate="20141229T011439Z">
        <seg>&lt;f0&gt;(&lt;/f0&gt;&lt;f1&gt;Purpose&lt;/f1&gt;&lt;f2&gt;)&lt;br3/&gt;</seg>
